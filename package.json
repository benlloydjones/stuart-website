{
  "name": "react-webpack-setup",
  "version": "1.0.0",
  "description": "A React and Webpack setup",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn build && node index",
    "start:client": "webpack-dev-server",
    "start:server": "nodemon",
    "build": "webpack -p",
    "seed": "node db/seeds",
    "test:client": "node_modules/.bin/_mocha test/client/helper \"test/client/**/*_test.js\"",
    "test:server": "node_modules/.bin/_mocha \"test/api/**/*_test.js\"",
    "test": "yarn test:server && yarn test:client"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bluebird": "^3.5.1",
    "express": "^4.16.2",
    "mongoose": "^5.0.3",
    "morgan": "^1.9.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "request": "^2.83.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.6.2",
    "mocha": "^5.0.0",
    "node-sass": "^4.7.2",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.6",
    "sinon": "^4.2.2",
    "style-loader": "^0.20.1",
    "supertest": "^3.0.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  }
}
